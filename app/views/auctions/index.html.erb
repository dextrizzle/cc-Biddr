<h1>biddr</h1>
<%= link_to 'new auction', new_auction_path %>
<br>
<%= link_to 'your bids', auction_bids_path(@auctions.last)%>

<h3>auctions</h3>
<% @auctions.each do |auction| %>
    <p><%= link_to auction.title, auction_path(auction) %>
      [<%= auction.aasm_state %>]
    <% if auction.user == current_user %>OWNER<%end%>
    </p>
<% end %>

<h3>watched auctions</h3>
<% @watched_auctions.each do |auction| %>
    <p><%= link_to auction.auction.title, auction_path(auction.auction) %>
      [<%= auction.auction.aasm_state %>]
      <% if auction.auction.user == current_user %>OWNER<%end%>
    </p>
<% end %>
